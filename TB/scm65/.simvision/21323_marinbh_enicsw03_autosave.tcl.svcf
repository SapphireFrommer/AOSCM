	} ]
waveform format $id -radix %b
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.decoder_write_out[15:0]}
	} ]
waveform format $id -radix %b
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.rwlBuffNet_0[15:0]}
	} ]
waveform format $id -radix %d
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.rwlBuffNet_1[15:0]}
	} ]
waveform format $id -radix %d
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.rwlBuffNet_2[15:0]}
	} ]
waveform format $id -radix %d
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.rwlBuffNet_3[15:0]}
	} ]
waveform format $id -radix %d
set id [waveform add -cdivider divider]
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.bitslice_1.DGWCLK[15:0]}
	} ]
waveform format $id -radix %b
set id [waveform add -signals  {
	simulator::tb_scm65.MEM.bitslice_1.DIN
	} ]
set id [waveform add -signals  {
	simulator::tb_scm65.MEM.bitslice_1.DOUT
	} ]
set id [waveform add -signals  {
	simulator::tb_scm65.MEM.bitslice_1.GDIN
	} ]
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.bitslice_1.MemoryLatch[15:0]}
	} ]
waveform format $id -radix %b
waveform hierarchy expand $id
set id [waveform add -signals  {
	{simulator::tb_scm65.MEM.bitslice_1.RWL[15:0]}
	} ]
waveform format $id -radix %b
waveform hierarchy collapse $id
set id [waveform add -signals  {
	simulator::tb_scm65.MEM.bitslice_1.clk
	} ]

waveform xview limits 0 120000ps

